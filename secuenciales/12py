valordea=int(input("valor a:"))
valordeb=int(input("valor b:"))
valordec=int(input("valor c:"))

valorb=valordea*valordec
psolucion=-(+valordec)
ssolucion=-(valordeb-valordec)

print( f"prima solucion={format(psolucion,'.2f')}" )
